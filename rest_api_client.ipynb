      "source": [
        "import requests\n",
        "import json\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "from MLModel import MLModel\n",
        "\n",
        "url = 'http://127.0.0.1:8000/model/predict'\n",
        "\n",
        "data_path = 'pipelines_with_mlflow_Docker\data\exam_results_train.csv'\n",
        "df = pd.read_csv(data_path)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "index = 100\n",
        "inference_row = df.iloc[index].apply(lambda x: x.item() if isinstance(x, np.generic) else x).tolist()\n",
        "\n",
        "data = {\n",
        "    \"inference_row\": pd.Series(inference_row).astype(str).to_list()\n",
        "}\n",
        "\n",
        "response = requests.post(url, json=data)\n",
        "\n",
        "print('Status Code:', response.status_code)\n",
        "print('Response Body:', response.text)"
      ]
    }
  ],
